{
  "name": "linkedin-profile-scraper",
  "version": "0.1.0",
  "description": "LinkedIn profile scraper returning structured profile data in JSON. Works in 2020.",
  "main": "",
  "scripts": {
    "test": "serverless-bundle test"
  },
  "author": "Jordy van den Aardweg <jordyvandenaardweg@gmail.com> (https://github.com/jvandenaardweg)",
  "dependencies": {
    "@serverless-chrome/lambda": "^1.0.0-57.2",
    "aws-lambda": "^1.0.6",
    "aws-sdk": "^2.987.0",
    "chrome-launcher": "^0.14.1",
    "dotenv": "^10.0.0",
    "i18n-iso-countries": "^5.4.0",
    "moment": "^2.27.0",
    "moment-timezone": "^0.5.31",
    "puppeteer": "^4.0.1",
    "superagent": "^6.1.0",
    "tree-kill": "^1.2.2",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.83",
    "@types/jest": "^26.0.3",
    "@types/moment-timezone": "^0.5.13",
    "@types/node": "^14.0.14",
    "@types/puppeteer": "^3.0.1",
    "jest": "^26.1.0",
    "release-it": "^13.6.4",
    "serverless-bundle": "^5.0.1",
    "serverless-dotenv-plugin": "^3.9.0",
    "serverless-offline": "^8.2.0",
    "serverless-plugin-chrome": "^1.0.0-55.3",
    "ts-jest": "^26.1.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript-tslint-plugin": "^0.5.5"
  },
  "jest": {
    "globals": {
      "NODE_ENV": "test"
    },
    "testEnvironment": "node",
    "setupFiles": [
      "dotenv/config"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.ts?$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ],
    "collectCoverageFrom": [
      "**/*.{ts,js,jsx,json,node}",
      "!**/node_modules/**",
      "!**/typings/**",
      "!**/integration/**"
    ],
    "testPathIgnorePatterns": [
      ".e2e.test.ts"
    ]
  }
}
